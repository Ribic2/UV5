<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/search_activity"
        tools:context=".Search" android:background="#FFFFFF">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="412dp"
            android:layout_height="526dp" tools:layout_editor_absoluteY="207dp" tools:layout_editor_absoluteX="0dp"
            android:backgroundTint="#FFFFFF"  android:translationZ="999dp">
    </LinearLayout>
    <ImageView
            android:layout_width="412dp"
            android:layout_height="276dp" tools:srcCompat="@drawable/plane"
            android:id="@+id/imageView" tools:layout_editor_absoluteX="-1dp" tools:layout_editor_absoluteY="0dp"
            android:scaleX="2" android:scaleY="2"/>
</androidx.constraintlayout.widget.ConstraintLayout>